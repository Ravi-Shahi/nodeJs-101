<%- include("header") -%>
    <div class="card">
        <form action="/" method="post">
            <input type="text" name="fname" placeholder="Enter your first name" autocomplete="off">
            <input type="text" name="lname" placeholder="Enter your last name" autocomplete="off">
            <input type="email" name="email" placeholder="Enter you email" autocomplete="off">
            <button type="submit">submit</button>
        </form>
    </div>
    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
        <% for(let i =0;i<data.length;i++){%>

            <tr>
                <%for(obj in data[i]){%>
                    <td>
                        <%=data[i][obj]%>
                    </td>
                    <%}%>

            </tr>
            <%}%>
    </table>

    <%- include("footer") -%>